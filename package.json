{
  "name": "class-converter",
  "version": "1.0.1",
  "description": "class converter",
  "main": "lib/index.js",
  "scripts": {
    "test": "nyc mocha -r ts-node/register tests/**/*.spec.ts",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier-check": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint --ext .ts src",
    "lint-fix": "eslint --ext .ts  src --fix",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run prettier && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zquancai/class-converter.git"
  },
  "keywords": [
    "class",
    "json",
    "mapper",
    "converter",
    "transformer",
    "typescript"
  ],
  "author": "zquancai@126.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zquancai/class-converter/issues"
  },
  "homepage": "https://github.com/zquancai/class-converter#readme",
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.2",
    "moment": "^2.24.0",
    "nyc": "^15.0.1",
    "prettier": "^1.19.1",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.3"
  },
  "files": [
    "lib",
    "tests"
  ]
